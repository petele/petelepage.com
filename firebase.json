{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash": true,
    "cleanUrls": true,
    "redirects": [{
        "source": "/blog/",
        "destination": "/",
        "type": 301
      }, {
        "source": "/blog",
        "destination": "/",
        "type": 301
      }, {
        "source": "/blog/index.php",
        "destination": "/",
        "type": 301
      }, {
        "source": "/about/biography/",
        "destination": "/about/",
        "type": 301
      }, {
        "source": "/blog/2013/08/raspberry-pi-quick-start/",
        "destination": "/blog/2021/08/rpi-setup/",
        "type": 301
      }
    ],
    "rewrites": [{
        "source": "/admin/",
        "destination": "/login/"
      }, {
        "source": "/wp-login.php",
        "destination": "/login/"
      }, {
        "source": "/wp-admin/",
        "destination": "/login/"
      }
    ]
  }
}
